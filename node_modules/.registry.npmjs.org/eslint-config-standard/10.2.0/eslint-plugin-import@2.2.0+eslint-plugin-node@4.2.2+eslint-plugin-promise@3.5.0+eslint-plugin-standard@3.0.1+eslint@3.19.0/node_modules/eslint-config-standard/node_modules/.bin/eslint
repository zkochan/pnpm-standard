#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint/3.19.0/node_modules/eslint/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint/3.19.0/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules:/home/zkochan/src/test/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint-config-standard/10.2.0/eslint-plugin-import@2.2.0+eslint-plugin-node@4.2.2+eslint-plugin-promise@3.5.0+eslint-plugin-standard@3.0.1+eslint@3.19.0/node_modules/eslint/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint-config-standard/10.2.0/eslint-plugin-import@2.2.0+eslint-plugin-node@4.2.2+eslint-plugin-promise@3.5.0+eslint-plugin-standard@3.0.1+eslint@3.19.0/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint-config-standard/10.2.0/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint-config-standard/node_modules" "$basedir/node"  "$basedir/../../../eslint/bin/eslint.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint/3.19.0/node_modules/eslint/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint/3.19.0/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules:/home/zkochan/src/test/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint-config-standard/10.2.0/eslint-plugin-import@2.2.0+eslint-plugin-node@4.2.2+eslint-plugin-promise@3.5.0+eslint-plugin-standard@3.0.1+eslint@3.19.0/node_modules/eslint/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint-config-standard/10.2.0/eslint-plugin-import@2.2.0+eslint-plugin-node@4.2.2+eslint-plugin-promise@3.5.0+eslint-plugin-standard@3.0.1+eslint@3.19.0/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint-config-standard/10.2.0/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint-config-standard/node_modules" node  "$basedir/../../../eslint/bin/eslint.js" "$@"
