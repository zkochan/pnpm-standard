#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/acorn/5.0.3/node_modules/acorn/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/acorn/5.0.3/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/acorn/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules:/home/zkochan/src/test/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/espree/3.4.1/node_modules/acorn/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/espree/3.4.1/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/espree/node_modules" "$basedir/node"  "$basedir/../../../acorn/bin/acorn" "$@"
  ret=$?
else 
  NODE_PATH="/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/acorn/5.0.3/node_modules/acorn/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/acorn/5.0.3/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/acorn/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules:/home/zkochan/src/test/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/espree/3.4.1/node_modules/acorn/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/espree/3.4.1/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/espree/node_modules" node  "$basedir/../../../acorn/bin/acorn" "$@"
  ret=$?
fi
exit $ret
