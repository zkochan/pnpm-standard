#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/rimraf/2.6.1/node_modules/rimraf/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/rimraf/2.6.1/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/rimraf/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules:/home/zkochan/src/test/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/del/2.2.2/node_modules/rimraf/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/del/2.2.2/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/del/node_modules" "$basedir/node"  "$basedir/../../../rimraf/bin.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/rimraf/2.6.1/node_modules/rimraf/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/rimraf/2.6.1/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/rimraf/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules:/home/zkochan/src/test/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/del/2.2.2/node_modules/rimraf/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/del/2.2.2/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/del/node_modules" node  "$basedir/../../../rimraf/bin.js" "$@"
  ret=$?
fi
exit $ret
