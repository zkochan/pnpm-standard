#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/shelljs/0.7.7/node_modules/shelljs/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/shelljs/0.7.7/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/shelljs/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules:/home/zkochan/src/test/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint/3.19.0/node_modules/shelljs/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint/3.19.0/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint/node_modules" "$basedir/node"  "$basedir/../../../shelljs/bin/shjs" "$@"
  ret=$?
else 
  NODE_PATH="/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/shelljs/0.7.7/node_modules/shelljs/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/shelljs/0.7.7/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/shelljs/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules:/home/zkochan/src/test/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint/3.19.0/node_modules/shelljs/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint/3.19.0/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint/node_modules" node  "$basedir/../../../shelljs/bin/shjs" "$@"
  ret=$?
fi
exit $ret
