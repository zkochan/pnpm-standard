#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/mkdirp/0.5.1/node_modules/mkdirp/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/mkdirp/0.5.1/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/mkdirp/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules:/home/zkochan/src/test/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/write/0.2.1/node_modules/mkdirp/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/write/0.2.1/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/write/node_modules" "$basedir/node"  "$basedir/../../../mkdirp/bin/cmd.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/mkdirp/0.5.1/node_modules/mkdirp/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/mkdirp/0.5.1/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/mkdirp/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules:/home/zkochan/src/test/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/write/0.2.1/node_modules/mkdirp/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/write/0.2.1/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/write/node_modules" node  "$basedir/../../../mkdirp/bin/cmd.js" "$@"
  ret=$?
fi
exit $ret
