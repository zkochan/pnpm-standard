#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint/3.19.0/node_modules/eslint/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint/3.19.0/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules:/home/zkochan/src/test/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint-plugin-node/4.2.2/eslint@3.19.0/node_modules/eslint/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint-plugin-node/4.2.2/eslint@3.19.0/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint-plugin-node/4.2.2/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint-plugin-node/node_modules" "$basedir/node"  "$basedir/../../../eslint/bin/eslint.js" "$@"
  ret=$?
else 
  NODE_PATH="/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint/3.19.0/node_modules/eslint/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint/3.19.0/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules:/home/zkochan/src/test/node_modules:/home/zkochan/src/node_modules:/home/zkochan/node_modules:/home/node_modules:/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint-plugin-node/4.2.2/eslint@3.19.0/node_modules/eslint/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint-plugin-node/4.2.2/eslint@3.19.0/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint-plugin-node/4.2.2/node_modules:/home/zkochan/src/test/pnpm-standard/node_modules/.registry.npmjs.org/eslint-plugin-node/node_modules" node  "$basedir/../../../eslint/bin/eslint.js" "$@"
  ret=$?
fi
exit $ret
